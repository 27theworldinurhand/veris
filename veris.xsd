<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:appinfo>
			<schema>VERIS Community</schema>
			<version>0.29</version>
			<date>23-04-2012</date>
		</xs:appinfo>
		<xs:documentation>Verizon Enterprise Risk and Incident Sharing (VERIS) framework. Terms of Use: Verizon grants you a limited, revocable, personal and nontransferable license to use the Verizon Enterprise Risk and Incident Sharing Framework for purposes of collecting, organizing and reporting security incident information for non-commercial purposes.  For purposes of this paragraph, “non-commercial purposes” includes organizing and reporting security incident information according to the framework, whether or not for compensation.  But “non-commercial” does not include any attempt to obtain compensation for providing access to, use of, or rights in the framework. </xs:documentation>
	</xs:annotation>
	<xs:element name="VERIS_community" type="VERISCommunity">
	</xs:element>
	<xs:complexType name="VERISCommunity">
		<xs:annotation>
			<xs:documentation>Repository of VERIS-C modeled incidents.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="incident" type="Incident" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="schema_version" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>schema version: The version of the VERIS-C schema this document is compliant with.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Incident">
		<xs:annotation>
			<xs:documentation>An incident is a collection of Agents, Actions, Assets, and Attributes focused around a single Victim. Do not confuse an Incident with a case which can involve many victims.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="incident_uid" type="xs:string">
				<xs:annotation>
					<xs:documentation>Incident or case ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="source_uid" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Handler name or ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="security_compromise" type="SECURITYCOMPROMISE">
				<xs:annotation>
					<xs:documentation>Was a security incident involved?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="related_incidents" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Related incidents</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="related_incident_uid" type="xs:string" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Related incident or case ID</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="summary" type="xs:string">
				<xs:annotation>
					<xs:documentation>Please provide a summary of the incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="xs:string">
				<xs:annotation>
					<xs:documentation>Include additional notes here. If multiple incidents were involved, estimate the total number and describe the circumstances.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="victim" type="Victim">
				<xs:annotation>
					<xs:documentation>Victim Demographics </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="agent" type="AgentAggregate" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Agents</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="action" type="ActionAggregate" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Which category of threat actions were invovled this incident? </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="assets" type="AssetAggregate">
				<xs:annotation>
					<xs:documentation>Assets</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attribute" type="AttributeAggregate" maxOccurs="unbounded" nillable="true">
				<xs:annotation>
					<xs:documentation>Which security attributes were compromised during this incident?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="discovery_method" type="DISCOVERYMETHOD">
				<xs:annotation>
					<xs:documentation>How was the incident discovered?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="control_failure" type="xs:string" minOccurs="0" maxOccurs="unbounded" nillable="true">
				<xs:annotation>
					<xs:documentation>What were the root control failures or weaknesses that allowed this incident to occur?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="corrective_action" type="xs:string" minOccurs="0" maxOccurs="unbounded" nillable="true">
				<xs:annotation>
					<xs:documentation>What corrective action(s) are planned (or recommended) to prevent and/or detect similar incidents in the future?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="loss" type="Loss" minOccurs="0" maxOccurs="unbounded" nillable="true">
				<xs:annotation>
					<xs:documentation>What varieties of losses were experienced as a result of this incident?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="impact_rating" type="IMPACTRATING" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How would you describe the overall impact of this incident to the organization?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="impact_estimate" type="Money" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provide a realistic estimate of total losses attributed to (or expected for) this incident.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certainty" type="CERTAINTY" minOccurs="0">
				<xs:annotation>
					<xs:documentation>How certain are you that the information you provided about this incident is accurate?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="first_action" type="DateTime" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>First known action</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="initial_compromise" type="DateTime" nillable="true"/>
			<xs:element name="data_exfiltration" type="DateTime" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data exfiltration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="incident_discovery" type="DateTime" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Incident discovery</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="containment" type="DateTime" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Containment / Restoration</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="investigation" type="DateTime">
				<xs:annotation>
					<xs:documentation>Marks the begining of the investigation.</xs:documentation>
				</xs:annotation>
			</xs:element>
            
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DateTime">
		<xs:annotation>
			<xs:documentation>A date of variable precision. Always assumed to be recorded in UTC. Dev note: TODO: Due to limitations in verisc data bindings these attributes are being represented as xs:string without xsd primitive validations.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="year" type="xs:string" />
			<xs:element name="month" type="xs:string" nillable="true"/>
			<xs:element name="day" type="xs:string" nillable="true"/>
			<xs:element name="time" type="xs:time" nillable="true"/>
            <!--
			<xs:element name="year" type="xs:gYear" />
			<xs:element name="month" type="xs:gMonth" nillable="true"/>
			<xs:element name="day" type="xs:gDay" nillable="true"/>
			<xs:element name="time" type="xs:time" nillable="true"/>
            -->
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Victim">
		<xs:annotation>
			<xs:documentation>Victim Demographics</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="uid" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Victim or organization ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="industry" type="Industry">
				<xs:annotation>
					<xs:documentation>Primary Industry</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="employee_count">
				<xs:annotation>
					<xs:documentation>Approximate number of employees: the size of the entire organization is preferred rather than the particular division, branch, location, etc affected. For an independent or individually-owned and operated franchise, however, the size of that particular franchise location is preferred.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1 to 10"/>
						<xs:enumeration value="11 to 100"/>
						<xs:enumeration value="101 to 1,000"/>
						<xs:enumeration value="1,001 to 10,000"/>
						<xs:enumeration value="10,001 to 25,000"/>
						<xs:enumeration value="25,001 to 50,000"/>
						<xs:enumeration value="50,001 to 100,000"/>
						<xs:enumeration value="Over 100,000"/>
						<xs:enumeration value="Unknown"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="location" type="Location">
				<xs:annotation>
					<xs:documentation>Country/region of operation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="revenue" type="Money" minOccurs="0" nillable="true">
				<xs:annotation>
					<xs:documentation>Annual revenue</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="security_budget" type="Money" minOccurs="0" nillable="true">
				<xs:annotation>
					<xs:documentation>Annual security budget</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Industry">
		<xs:annotation>
			<xs:documentation>Primary Industry (Note: VERIS uses standard NAIC codes for the list of industries. NAIC provides descriptions for all listings at http://www.census.gov/cgi-bin/sssd/naics/naicsrch?chart=2007. If multiple options apply, select the primary industry of the business group that experienced the incident.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="\d{3,}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Location">
		<xs:annotation>
			<xs:documentation>Country / region of operation</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="country" type="COUNTRY">
				<xs:annotation>
					<xs:documentation>Courntry</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="region" type="REGION">
				<xs:annotation>
					<xs:documentation>Region</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Money">
		<xs:annotation>
			<xs:documentation>Amount and currency</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Amount</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="iso_currency_code" default="USD">
				<xs:annotation>
					<xs:documentation>ISO 4217 currency code if other than USD </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
    <xs:complexType name="AgentAggregate">
		<xs:annotation>
			<xs:documentation>Agent</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="motive" type="MOTIVE" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>What motive drove the agent(s) to act?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="role" type="ROLE" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>What role did the external agent(s) play in this incident? </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="notes" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Enter any additional details you deem noteworthy about the agent(s).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="External">
		<xs:annotation>
			<xs:documentation>External agent</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AgentAggregate">
				<xs:sequence>
					<xs:element name="varieties" type="EXTERNALVARIETY" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What type of external agent was involved? </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="origins" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>What is the geographic origin of the external agent(s)? </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="origin" type="Location" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Geographic origin </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ips" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Enter any IP addresses associated with external agent(s).</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ip" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>IP address</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Internal">
		<xs:annotation>
			<xs:documentation>Internal agent</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AgentAggregate">
				<xs:sequence>
					<xs:element name="varieties" type="INTERNALVARIETY" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What type of internal agent was involved? </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Partner">
		<xs:annotation>
			<xs:documentation>Partner agent</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="AgentAggregate">
				<xs:sequence>
					<xs:element name="industry" type="Industry" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Which industry best describes the services provided by the partner(s)? </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="origins" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>What is the geographic origin of the partner(s)? </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="origin" type="Location" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Geographic origin </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ActionAggregate">
		<xs:annotation>
			<xs:documentation>Action</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="notes" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Enter any additional details you deem noteworthy about the action(s).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Malware">
		<xs:annotation>
			<xs:documentation>Malware</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ActionAggregate">
				<xs:sequence>
					<xs:element name="varieties" type="MALWAREVARIETY" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What functions or varieties of malware were involved?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="vectors" type="MALWAREVECTOR" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What was the vector or path of infection?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cves" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Enter any CVEs exploited by this malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="cve" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>CVE exploited by this malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="names" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Strain or common names of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="name" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Strain or common name of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="filenames" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Particular filenames of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="filename" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Particular filename of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="hash_values" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Hashes of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="hash_value" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Hash value of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ips" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Outbound IP addresses of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ip" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Outbound IP address of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
                    <xs:element name="urls" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Outbound URLs of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="url" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Outbound URL of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
                    <xs:element name="domains" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Outbound URLs of malware</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="domain" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>Outbound URL of malware</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Hacking">
		<xs:annotation>
			<xs:documentation>Hacking</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ActionAggregate">
				<xs:sequence>
					<xs:element name="varieties" type="HACKINGVARIETY" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What methods or varieties of hacking were involved?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="vectors" type="HACKINGVECTOR" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What vectors or paths did these hacks exploits?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="cves" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Enter any CVEs exploited through hacking</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="cve" type="xs:string" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>CVE exploited through hacking</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Social">
		<xs:annotation>
			<xs:documentation>Social</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ActionAggregate">
				<xs:sequence>
					<xs:element name="varieties" type="SOCIALVARIETY" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What varieties of social tactics were involved?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="vectors" type="SOCIALVECTOR" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>What communication channels were used?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="email" type="EmailAggregate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Information on emails utlizing social tactics</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="social_target" type="SOCIALTARGET" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
